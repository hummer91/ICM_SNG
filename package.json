{
  "name": "html5-javascript-project",
  "version": "1.0.0",
  "description": "HTML5 + JavaScript 프론트엔드 프로젝트",
  "main": "index.html",
  "scripts": {
    "start": "npx live-server --port=3000 --open=/",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,css,html}\""
  },
  "keywords": ["html5", "javascript", "css3", "frontend"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "live-server": "^1.2.2"
  }
}